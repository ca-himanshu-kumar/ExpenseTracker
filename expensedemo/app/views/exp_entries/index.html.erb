<p id="notice"><%= notice %></p>

<h1>Exp Entries</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Group</th>
      <th>Billno</th>
      <th>Amount</th>
      <th>Billdate</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @exp_entries.each do |exp_entry| %>
      <tr>
        <td><%= exp_entry.title %></td>
        <td><%= exp_entry.group %></td>
        <td><%= exp_entry.billNo %></td>
        <td><%= exp_entry.amount %></td>
        <td><%= exp_entry.BillDate %></td>
        <td><%= exp_entry.user_id %></td>
        <td><%= link_to 'Show', exp_entry %></td>
        <td><%= link_to 'Edit', edit_exp_entry_path(exp_entry) %></td>
        <td><%= link_to 'Destroy', exp_entry, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Exp Entry', new_exp_entry_path %>

<%= link_to 'Back', 'home' %>
